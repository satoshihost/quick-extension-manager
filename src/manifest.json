{
  "manifest_version": 3,
  "name": "Quick Extension Manager",
  "version": "1.0.2",
  "description": "Quickly enable/disable individual browser extensions from a popup",
  "permissions": [
    "management",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Quick Extension Manager"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}